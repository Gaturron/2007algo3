BODY {
	BACKGROUND: white; COLOR: black; FONT-FAMILY: sans-serif
}
P.title1 {
	FONT-WEIGHT: bold; MARGIN-LEFT: 0em; TEXT-ALIGN: center
}
P.title2 {
	PADDING-BOTTOM: 1.5em; MARGIN-LEFT: 0em; TEXT-TRANSFORM: uppercase; =
TEXT-ALIGN: center
}
H1 {
	COLOR: purple; PADDING-TOP: 0.5em
}
H2 {
	COLOR: teal; PADDING-TOP: 0.5em
}
H3 {
	COLOR: olive; PADDING-TOP: 0.5em
}
DIV {
	MARGIN-LEFT: 2em
}
DIV.example {
	PADDING-LEFT: 1em; MARGIN-LEFT: 1em; BORDER-LEFT: thin
}
P.example {
	MARGIN-LEFT: 2em
}
P.definition {
	FONT-WEIGHT: bold; MARGIN-LEFT: 5em; MARGIN-RIGHT: 5em
}
P.equation {
	MARGIN-LEFT: 2em
}
.question {
	FONT-WEIGHT: bold; COLOR: purple; TEXT-ALIGN: center
}
IMG.example {
	TEXT-ALIGN: center
}
IMG.graph {
	TEXT-ALIGN: center
}
OL LI {
	FONT-WEIGHT: bold; MARGIN: 2ex 0ex 0ex
}
OL {
	MARGIN-TOP: 3ex; MARGIN-BOTTOM: 3ex; LIST-STYLE-TYPE: upper-roman
}
OL OL {
	MARGIN-TOP: 3ex; MARGIN-BOTTOM: 3ex; LIST-STYLE-TYPE: lower-roman
}
.important {
	BORDER-RIGHT: red thin double; PADDING-RIGHT: 0.5em; BORDER-TOP: red =
